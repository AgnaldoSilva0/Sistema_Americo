<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="541.0" prefWidth="698.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.TelaCarregarOrcamentoController">
   <children>
      <Button fx:id="btCarregar" layoutX="216.0" layoutY="502.0" mnemonicParsing="false" onAction="#onBtCarregarOrcamento" prefHeight="25.0" prefWidth="277.0" style="-fx-background-color: AQUA;" text="Carregar" textFill="WHITE">
         <font>
            <Font name="Arial Bold" size="13.0" />
         </font></Button>
      <Button fx:id="btPesquisar" layoutX="190.0" layoutY="21.0" mnemonicParsing="false" style="-fx-background-color: AQUA;" text="Pesquisar" textFill="WHITE">
