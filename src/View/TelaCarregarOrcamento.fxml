            <Font name="System Bold" size="12.0" />
         </font></Button>
      <TextField fx:id="tfPesquisar" layoutX="14.0" layoutY="21.0" prefHeight="25.0" prefWidth="176.0" promptText="Fonte" />
      <TableView fx:id="tbOrcamento" layoutX="14.0" layoutY="57.0" prefHeight="438.0" prefWidth="678.0">
        <columns>
            <TableColumn fx:id="clmTeste" prefWidth="68.0" text="Nº Pedido" />
          <TableColumn fx:id="clmCliente" prefWidth="453.0" text="Cliente" />
            <TableColumn fx:id="clmData" prefWidth="143.0" text="Data" />
          <TableColumn fx:id="clmnPedido" minWidth="8.0" prefWidth="12.0" text="T" />
        </columns>
      </TableView>
      <Button fx:id="btAtualizar" layoutX="43.0" layoutY="503.0" mnemonicParsing="false" onAction="#onBtAtualizarLista" style="-fx-background-color: aqua;" text="Atualizar Lista" textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <ImageView fx:id="imgAtualizar" fitHeight="25.0" fitWidth="29.0" layoutX="14.0" layoutY="503.0" pickOnBounds="true" preserveRatio="true" />
      <Button fx:id="btSair" layoutX="596.0" layoutY="503.0" mnemonicParsing="false" onAction="#onBtSair" prefHeight="25.0" prefWidth="96.0" style="-fx-background-color: aqua;" text="Sair">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <ImageView fx:id="imgSair" fitHeight="25.0" fitWidth="29.0" layoutX="567.0" layoutY="503.0" pickOnBounds="true" preserveRatio="true" />
      <Label layoutX="522.0" layoutY="1.0" prefHeight="27.0" prefWidth="176.0" text="Carregar Orçamento" textFill="#395d5e" underline="true">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
